    "    if not use_qpu:\n",
    "        qc.depolarizing([qreg[0],qreg[1]],p_depol)\n",
    "    qc.cc_x([qreg[1], qreg[0]])\n",
    "    if not use_qpu:\n",
    "        qc.depolarizing([qreg[0],qreg[1]],p_depol)\n",
    "    \n",
    "    if use_qpu: \n",
    "        qc = Circuit().add_verbatim_box(qc)\n",
    "\n",
    "    result = qd.run(qc, shots=N_s, **kw).result()\n",
    "\n",
    "    print(qc)"
   ]
